// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  userId    Int      @id
  email     String   @unique
  firstName String
  lastName  String
  age       Int
  plan      String
  createdAt DateTime @unique @default(now())
}

model Course {
  id       Int    @id @default(autoincrement())
  campus   String @default("UBCV")
  subject  String
  course   Int
  courseId String @unique
  title    String
  // offering Offering[]
}

model Offering {
  id            Int     @id @default(autoincrement())
  courseId      String
  year          Int
  term          String
  section       String
  professor     String? @db.VarChar(1500)
  enrolled      Int
  avg           Float?
  high          Float?
  low           Float?
  lessThan50    Float?
  between50_54  Float?
  between55_59  Float?
  between60_63  Float?
  between64_67  Float?
  between68_71  Float?
  between72_75  Float?
  between76_79  Float?
  between80_84  Float?
  between85_89  Float?
  between90_100 Float?

  @@index([courseId])
}
